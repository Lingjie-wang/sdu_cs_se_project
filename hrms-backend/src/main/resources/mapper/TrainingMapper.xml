<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gump.hrbackend.mapper.TrainingMapper">

    <resultMap id="BaseResultMap" type="com.gump.hrbackend.model.entity.Training">
        <id property="id" column="id" jdbcType="INTEGER"/>
        <result property="empId" column="emp_id" jdbcType="INTEGER"/>
        <result property="empName" column="emp_name" jdbcType="VARCHAR"/>
        <result property="trainingProject" column="training_project" jdbcType="VARCHAR"/>
        <result property="trainingDate" column="training_date" jdbcType="TIMESTAMP"/>
        <result property="trainingDuration" column="training_duration" jdbcType="INTEGER"/>
        <result property="trainer" column="trainer" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id
        ,emp_id,emp_name,
        training_project,training_date,training_duration,
        trainer
    </sql>
</mapper>
